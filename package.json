{
  "name": "vite-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:production": "tsc && vite build --mode  production",
    "build:staging": "tsc && vite build --mode staging",
    "compress": "node tinyfy.js",
    "dev": "vite --host",
    "gencode": "graphql-codegen --config codegen.yml",
    "lint": "npm run lint:js && tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "prepare": "husky install",
    "prettier": "prettier --check --write \"src/**/*\"",
    "preview": "vite preview"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@apollo/client": "3.7.1",
    "@chakra-ui/react": "^2.4.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "ahooks": "^3.7.4",
    "axios": "^1.3.1",
    "bignumber.js": "^9.1.1",
    "dayjs": "^1.11.7",
    "framer-motion": "^8.5.4",
    "graphql": "^16.6.0",
    "less": "^4.1.3",
    "p-limit": "^4.0.0",
    "rc-pagination": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-jazzicon": "^1.0.4",
    "react-photo-view": "^1.2.3",
    "react-router-dom": "^6.8.0",
    "react-select": "^5.7.0",
    "video-react": "^0.16.0",
    "web3": "^1.8.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/client-preset": "^2.1.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/babel__core": "^7.1.17",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^14.14.37",
    "@types/numeral": "^2.0.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/tiny-async-pool": "^2.0.0",
    "@types/video-react": "^0.15.1",
    "@typescript-eslint/parser": "^5.10.0",
    "@umijs/fabric": "^2.5.6",
    "@vitejs/plugin-react": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": ">=6",
    "lint-staged": ">=10",
    "lodash-es": "^4.17.21",
    "numeral": "^2.0.6",
    "tinify": "^1.7.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-compression": "^0.5.1"
  },
  "engines": {
    "node": ">=12.2.0"
  }
}
